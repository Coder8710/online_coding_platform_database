
CREATE TABLE User_normalized (
    user_id INT PRIMARY KEY,
    Name VARCHAR(100) NOT NULL,
    email_id VARCHAR(100) NOT NULL,
    password VARCHAR(100) NOT NULL,
    total_problems INT
);

-- Create User_Subscription table to link users and subscriptions
CREATE TABLE User_Subscription (
    user_id INT NOT NULL,
    sub_id INT NOT NULL,
    PRIMARY KEY (user_id, sub_id),
    FOREIGN KEY (user_id) REFERENCES User(user_id),
    FOREIGN KEY (sub_id) REFERENCES Subscription(sub_id)
);

-- Insert data into User_normalized table
INSERT INTO User_normalized (user_id, Name, email_id, password, total_problems) VALUES
